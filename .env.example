# .env.example - 环境变量模板
# 复制此文件为 .env 并填入实际值

# 数据库配置
DATABASE_URL=sqlite:///./option_tracker.db

# Redis配置（可选）
REDIS_URL=redis://localhost:6379/0

# 交易可查账号（必填）
JYK_USER=your_phone_number
JYK_PASS=your_password

# Gemini API配置（必填）
GEMINI_API_KEY=your_api_key
GEMINI_BASE_URL=https://www.apillm.online/v1

# 应用配置
DEBUG=False
LOG_LEVEL=INFO
